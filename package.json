{
  "name": "zu-cesium-basemap",
  "version": "1.0.2",
  "description": "cesium底图服务",
  "keywords": [
    "cesium",
    "tianditu",
    "cesium-map",
    "cesium basemap"
  ],
  "private": false,
  "author": "zulezhe",
  "license": "MIT",
  "main": "dist/zu-cesium-basemap.umd.js",
  "module": "dist/zu-cesium-basemap.esm.js",
  "unpkg": "dist/zu-cesium-basemap.min.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development rollup --config build/rollup.config.es.js --watch",
    "build": "cross-env NODE_ENV=production npm run build:browser && npm run build:es && npm run build:umd",
    "test": "npm run test:unit",
    "build:browser": "rollup --config build/rollup.config.browser.js",
    "build:es": "rollup --config build/rollup.config.es.js",
    "build:umd": "rollup --config build/rollup.config.umd.js",
    "doc": "jsdoc -c ./docs/conf.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zulezhe/zu-cesium-basemap.git"
  },
  "bugs": {
    "url": "https://github.com/zulezhe/zu-cesium-basemap/issues"
  },
  "homepage": "https://github.com/zulezhe/zu-cesium-basemap",
  "dependencies": {
    "jsdoc": "^3.6.7"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-stage-2": "^7.8.3",
    "babel": "^6.23.0",
    "babel-loader": "^8.1.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "cross-env": "^7.0.2",
    "docdash": "^1.2.0",
    "rollup": "^2.28.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
